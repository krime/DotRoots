[user]
	name = kings
	email = kings@mars
[core]
	excludesfile = $HOME/.gitignore_global
	editor = vim
	eol = lf
	logallrefupdates = true
[difftool "sourcetree"]
	cmd = vimdiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = vimdiff \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[branch]
	autosetuprebase = always
[branch "master"]
	rebase = true
[color]
	status = always
	ui = always
[merge]
	conflictstyle = diff3
	tool = vimdiff
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
  meta = yellow
  frag = cyan
  old = white dim
  new = green
[color "status"]
	added = green
  changed = red dim bold
	untracked = yellow bold
[push]
	default = current
[http]
	sslVerify = false
